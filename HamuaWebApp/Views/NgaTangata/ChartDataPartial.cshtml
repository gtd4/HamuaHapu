@model HamuaHapuRegistration.ViewModels.ChartDataViewModel;

<div id="@Model.Title.Replace(" ", "-")-collapse">
    <div class="">
        <div class="" id="@Model.Title.Replace(" ", "-")-heading">
            <h5 class="mb-0">
                <button class="btn btn-link" data-toggle="collapse" data-target="#@Model.Title.Replace(" ", "-")" aria-expanded="false" aria-controls="@Model.Title.Replace(" ", "-")">
                    @Model.Title
                </button>
            </h5>
        </div>

        <div id="@Model.Title.Replace(" ", "-")" class="collapse" aria-labelledby="@Model.Title.Replace(" ", "-")-heading" data-parent="#@Model.Title.Replace(" ", "-")-collapse">
            <div class="card-body">
                <canvas id="myChart-@Model.Title.Replace(" ", "-")">
                </canvas>
            </div>
        </div>
    </div>
</div>